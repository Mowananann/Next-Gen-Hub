Section6:NewTextBox("Hitbox modifier", "cool thing", function(txt)
    _G.HeadSize = tonumber(txt)
    _G.Disabled = false  -- Ensure this is set to false so the function can run

    game:GetService('RunService').RenderStepped:Connect(function()
        if not _G.Disabled then  -- Use 'not' to check if it's false
            for i, v in ipairs(game:GetService('Players'):GetPlayers()) do
                if v.Name ~= game:GetService('Players').LocalPlayer.Name then
                    pcall(function()
                        v.Character.HumanoidRootPart.Size = Vector3.new(_G.HeadSize, _G.HeadSize, _G.HeadSize)
                        v.Character.HumanoidRootPart.Transparency = 0.7
                        v.Character.HumanoidRootPart.BrickColor = BrickColor.new("Really black")
                        v.Character.HumanoidRootPart.Material = Enum.Material.Neon
                        v.Character.HumanoidRootPart.CanCollide = false
                    end)
                end
            end
        end
    end)
end)
