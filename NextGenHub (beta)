end)
tool.Parent = game.Players.LocalPlayer.Backpack
end)
Section9:NewLabel("Advance Player stuff")
local player = game.Players.LocalPlayer
local positionLabel

local function updatePosition(character)
    if character and character:FindFirstChild("HumanoidRootPart") then
        local currentPosition = character:WaitForChild("HumanoidRootPart").Position
        positionLabel:UpdateLabel("Current Position: " .. "X: " .. math.floor(currentPosition.X) .. " Y: " .. math.floor(currentPosition.Y) .. " Z: " .. math.floor(currentPosition.Z))
    else
        positionLabel:UpdateLabel("Character is dead or not loaded.")
    end
end

local function onCharacterAdded(character)
    local RunService = game:GetService("RunService")
    local interval = 1 -- 1 second interval
    local elapsedTime = 0

    RunService.Heartbeat:Connect(function(step)
        elapsedTime = elapsedTime + step
        if elapsedTime >= interval then
            updatePosition(player.Character)
            elapsedTime = 0
        end
    end)
end


positionLabel = Section9:NewLabel("Current Position: Initializing...")


player.CharacterAdded:Connect(onCharacterAdded)


if player.Character then
    onCharacterAdded(player.Character)
end

Section9:NewButton("Copy Current Position", "Hey", function()
    if player.Character then
        local currentPosition = player.Character:WaitForChild("HumanoidRootPart").Position
        setclipboard("Position: X: " .. math.floor(currentPosition.X) .. " Y: " .. math.floor(currentPosition.Y) .. " Z: " .. math.floor(currentPosition.Z))
    end
end)
Section9:NewTextBox("Enter Position", "Format: X,Y,Z", function(txt)
    
    local coordinates = {}
    for coord in string.gmatch(txt, "([^,]+)") do
        table.insert(coordinates, tonumber(coord))
    end

    
    if #coordinates == 3 then
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

        
        humanoidRootPart.CFrame = CFrame.new(coordinates[1], coordinates[2], coordinates[3])
    else
        warn("Please enter a valid position in the format: X,Y,Z")
    end
end)

Section9:NewLabel("Other Players")

local Players = game:GetService("Players")
local playerOptions = {}
local selectedPlayer = nil
local isSpectating = false

local dropdown = Section9:NewDropdown("Players", "List of players", playerOptions, function(currentOption)
    
    local username = string.match(currentOption, "([^/]+)")
    selectedPlayer = Players:FindFirstChild(username)
    if isSpectating and selectedPlayer then
        
        game.Workspace.CurrentCamera.CameraSubject = selectedPlayer.Character
    end
end)

local toggle = Section9:NewToggle("Spectate (kinda laggy)", "Toggle spectating the selected player", function(state)
    isSpectating = state
    if isSpectating and selectedPlayer then
        print("Spectate On")
        
        game.Workspace.CurrentCamera.CameraSubject = selectedPlayer.Character
    else
        print("Spectate Off")
        
        local localPlayer = Players.LocalPlayer
        if localPlayer and localPlayer.Character then
            game.Workspace.CurrentCamera.CameraSubject = localPlayer.Character
        end
    end
end)

local button = Section9:NewButton("Teleport", "Teleport to the selected player", function()
    if selectedPlayer then
        local localPlayer = Players.LocalPlayer
        if localPlayer and localPlayer.Character then
            local humanoidRootPart = localPlayer.Character:FindFirstChild("HumanoidRootPart")
            local selectedPlayerCharacter = selectedPlayer.Character
            if humanoidRootPart and selectedPlayerCharacter then
                
                humanoidRootPart.CFrame = selectedPlayerCharacter.HumanoidRootPart.CFrame
            end
        end
    end
end)



local function updateDropdown()
    
    for i = #playerOptions, 1, -1 do
        table.remove(playerOptions, i)
    end

    
    for _, player in ipairs(Players:GetPlayers()) do
        table.insert(playerOptions, player.Name .. "/" .. player.DisplayName)
    end

    
    dropdown:Refresh(playerOptions)
end


updateDropdown()


Players.PlayerAdded:Connect(function()
    updateDropdown()
end)

Players.PlayerRemoving:Connect(function()
    updateDropdown()
end)

local flingButton = Section9:NewButton("Fling Player", "Fling the selected player", function()
    if selectedPlayer then
        local character = selectedPlayer.Character
        if character and character:FindFirstChild("HumanoidRootPart") then
            local rootPart = character.HumanoidRootPart
            -- Remove any existing BodyVelocity to avoid conflicts
            if rootPart:FindFirstChild("FlingVelocity") then
                rootPart.FlingVelocity:Destroy()
            end
            
            -- Create a new BodyVelocity object to apply the fling force
            local bodyVelocity = Instance.new("BodyVelocity")
            bodyVelocity.Name = "FlingVelocity"
            bodyVelocity.Velocity = Vector3.new(0, 500, 0) -- Adjust the vector as needed
            bodyVelocity.P = 1250
            bodyVelocity.MaxForce = Vector3.new(100000, 100000, 100000)
            bodyVelocity.Parent = rootPart

            -- Remove the BodyVelocity after a short duration
            game:GetService("Debris"):AddItem(bodyVelocity, 0.1)

            print("Flung player: " .. selectedPlayer.Name)
        else
            print("Selected player's character or HumanoidRootPart not found.")
        end
    else
        print("No player selected.")
    end
end)

