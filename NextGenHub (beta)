-- Open/close Gui

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "ScreenGui"
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ResetOnSpawn = false

-- Define sound effects
local firstClickSound = Instance.new("Sound")
firstClickSound.Name = "FirstClickSound"
firstClickSound.SoundId = "rbxassetid://907514098" -- Replace with your first sound ID
firstClickSound.Parent = ScreenGui

local secondClickSound = Instance.new("Sound")
secondClickSound.Name = "SecondClickSound"
secondClickSound.SoundId = "rbxassetid://907514098" -- Replace with your second sound ID
secondClickSound.Parent = ScreenGui

-- Create the logo ImageButton
local Logo = Instance.new("ImageButton")
Logo.Name = "Logo"
Logo.Parent = ScreenGui
Logo.BackgroundColor3 = Color3.new(1, 1, 1) -- White background
Logo.BackgroundTransparency = 1 -- Fully transparent background
Logo.Position = UDim2.new(0, 0, 0, 0) -- Top-left corner of the screen
Logo.Size = UDim2.new(0, 50, 0, 50) -- Square size
Logo.Image = "rbxassetid://YOUR_IMAGE_ID" -- Replace with your logo image asset ID

-- Add rounded corners to the logo
local Corner = Instance.new("UICorner")
Corner.Name = "Corner"
Corner.CornerRadius = UDim.new(0.1, 0) -- Rounded corners
Corner.Parent = Logo

-- Create the toggle button
local Toggle = Instance.new("TextButton")
Toggle.Name = "Toggle"
Toggle.Parent = ScreenGui
Toggle.BackgroundColor3 = Color3.new(0, 0, 0)
Toggle.Position = UDim2.new(0, 0, 0.4547, 0) -- Position below the logo
Toggle.Size = UDim2.new(0, 50, 0, 50) -- Square size
Toggle.Text = "-"
Toggle.TextColor3 = Color3.new(1, 1, 1)
Toggle.TextScaled = true
Toggle.Font = Enum.Font.SourceSans
Toggle.ClipsDescendants = true
Toggle.Draggable = true

-- Define toggle state
local isGuiOpen = true

-- Toggle functionality with sounds
Toggle.MouseButton1Click:Connect(function()
    if not Toggle:IsDescendantOf(game.Players.LocalPlayer:WaitForChild("PlayerGui")) then
        return
    end
    
    if isGuiOpen then
        firstClickSound:Play()
        Toggle.Text = "+"
        isGuiOpen = false
        -- Replace Library:ToggleUI() with the actual functionality you want
        -- Library:ToggleUI() -- Assuming Library is defined elsewhere
        
    else
        secondClickSound:Play()
        Toggle.Text = "-"
        isGuiOpen = true
        -- Replace Library:ToggleUI() with the actual functionality you want
        -- Library:ToggleUI() -- Assuming Library is defined elsewhere
    end
end)

-- Add rounded corners to the toggle button
local CornerToggle = Instance.new("UICorner")
CornerToggle.Name = "Corner"
CornerToggle.CornerRadius = UDim.new(0.1, 0) -- Rounded corners
CornerToggle.Parent = Toggle

-- Destroy the script's parent Library, if defined
if script.Parent then
    script.Parent:Destroy()
end
