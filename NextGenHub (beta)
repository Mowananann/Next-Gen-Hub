local playerOptions = {}
local selectedPlayer = nil

local dropdown = Section1:NewDropdown("Players", "List of players", playerOptions, function(currentOption)
    
    local username = string.match(currentOption, "([^/]+)")
    selectedPlayer = Players:FindFirstChild(username)
end)


local button = Section1:NewButton("Copy Username", "Copy the username of the selected player", function()
    if selectedPlayer then
        
        setclipboard("Username: " .. selectedPlayer.Name)
        print("Username copied to clipboard: " .. selectedPlayer.Name)
    else
        print("No player selected.")
    end
end)

local button = Section1:NewButton("Copy Display Name", "Copy the display name of the selected player", function()
    if selectedPlayer then
        
        setclipboard("Display Name: " .. selectedPlayer.DisplayName)
        print("Display name copied to clipboard: " .. selectedPlayer.DisplayName)
    else
        print("No player selected.")
    end
end)

local button = Section1:NewButton("Copy Player id", "Copy the Id of the selected player", function()
    if selectedPlayer then
        
        setclipboard("Id: " .. selectedPlayer.UserId)
        print("Id copied to clipboard: " .. selectedPlayer.UserId)
    else
        print("No player selected.")
    end
end)


local function updateDropdown()
    
    for i = #playerOptions, 1, -1 do
        table.remove(playerOptions, i)
    end

    
    for _, player in ipairs(Players:GetPlayers()) do
        table.insert(playerOptions, player.Name .. "/" .. player.DisplayName)
    end

    
    dropdown:Refresh(playerOptions)
end


updateDropdown()


Players.PlayerAdded:Connect(function()
    updateDropdown()
end)

Players.PlayerRemoving:Connect(function()
    updateDropdown()
end)


