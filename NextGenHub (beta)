Section8:NewLabel("More stuff")
Section8:NewButton("Modded click to tp tool", "Wsg", function()
    local mouse = game.Players.LocalPlayer:GetMouse()
local tool = Instance.new("Tool")
tool.RequiresHandle = false
tool.Name = "Equip to Click TP"

local sound = Instance.new("Sound")
sound.SoundId = "rbxassetid://5433025904"
sound.Volume = 1
sound.Parent = game.Workspace

tool.Activated:Connect(function()
    local startPos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
    local endPos = mouse.Hit.p
    local undergroundPos = endPos - Vector3.new(0, 5, 0) 
    local groundPos = endPos + Vector3.new(0, 5, 0) 
    local avatar = game.Players.LocalPlayer.Character
    local rootPart = avatar:FindFirstChild("HumanoidRootPart")
    rootPart.CFrame = CFrame.new(undergroundPos)
    rootPart.Transparency = 1 
    wait(0.5) 
    local tweenInfo = TweenInfo.new(2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
    local tween = game.TweenService:Create(rootPart, tweenInfo, {CFrame = CFrame.new(groundPos)})
    tween:Play()
    tween.Completed:Connect(function()
        rootPart.Transparency = 0 
        sound:Play() 
    end)
end)
tool.Parent = game.Players.LocalPlayer.Backpack
end)

