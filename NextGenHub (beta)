        Text = "Enjoy ";
        Icon = "rbxthumb://type=Asset&id=11602284370&w=150&h=150";
        Duration = 10; -- set duration in seconds
        Button1 = "IT DID LOAD";
        Button2 = "IT DIDNT LOAD";
    })
end)
