local player = game.Players.LocalPlayer
local positionLabel

local function updatePosition(character)
    if character and character:FindFirstChild("HumanoidRootPart") then
        local currentPosition = character:WaitForChild("HumanoidRootPart").Position
        positionLabel:UpdateLabel("Current Position: " .. "X: " .. math.floor(currentPosition.X) .. " Y: " .. math.floor(currentPosition.Y) .. " Z: " .. math.floor(currentPosition.Z))
    else
        positionLabel:UpdateLabel("Character is dead or not loaded.")
    end
end

local function onCharacterAdded(character)
    local RunService = game:GetService("RunService")
    local interval = 1 
    local elapsedTime = 0

    RunService.Heartbeat:Connect(function(step)
        elapsedTime = elapsedTime + step
        if elapsedTime >= interval then
            updatePosition(player.Character)
            elapsedTime = 0
        end
    end)
end


positionLabel = Section6:NewLabel("Current Position: Initializing...")


player.CharacterAdded:Connect(onCharacterAdded)


if player.Character then
    onCharacterAdded(player.Character)
end

Section6:NewButton("Copy Current Position", "Hey", function()
    if player.Character then
        local currentPosition = player.Character:WaitForChild("HumanoidRootPart").Position
        setclipboard("Position: X: " .. math.floor(currentPosition.X) .. " Y: " .. math.floor(currentPosition.Y) .. " Z: " .. math.floor(currentPosition.Z))
    end
end)
Section6:NewTextBox("Enter Position", "Format: X,Y,Z", function(txt)
    
    local coordinates = {}
    for coord in string.gmatch(txt, "([^,]+)") do
        table.insert(coordinates, tonumber(coord))
    end

    
    if #coordinates == 3 then
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

        
        humanoidRootPart.CFrame = CFrame.new(coordinates[1], coordinates[2], coordinates[3])
    else
        warn("Please enter a valid position in the format: X,Y,Z")
    end
end)

Section6:NewLabel("i made these exept the aimbot for pc")
local InfiniteJumpEnabled = false

local UserInputService = game:GetService("UserInputService")
local Players = game:GetService("Players")

local function onJumpRequest()
    if InfiniteJumpEnabled then
        local character = Players.LocalPlayer.Character
        if character then
            local humanoid = character:FindFirstChildOfClass('Humanoid')
            if humanoid then
                humanoid:ChangeState(Enum.HumanoidStateType.Physics)
                humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
        end
    end
end

UserInputService.JumpRequest:Connect(onJumpRequest)

Section6:NewToggle("Infinite Jump", "By toggling this you can jump infinitely.", function(state)
    InfiniteJumpEnabled = state
    if state then
        print("Infinite Jump: ON")
    else
        print("Infinite Jump: OFF")
    end
end)

-- Initialize global variables
_G.HeadSize = 5  -- Default head size
_G.Disabled = false  -- Toggle state for hitbox visibility

-- Services
local Players = game:GetService('Players')
local RunService = game:GetService('RunService')

-- Update hitbox size and properties
local function updateHitbox()
    for _, player in ipairs(Players:GetPlayers()) do
        if player.Name ~= Players.LocalPlayer.Name then
            local character = player.Character
            if character and character:FindFirstChild('HumanoidRootPart') then
                local humanoidRootPart = character.HumanoidRootPart
                humanoidRootPart.Size = Vector3.new(_G.HeadSize, _G.HeadSize, _G.HeadSize)
                humanoidRootPart.Transparency = 0.7
                humanoidRootPart.BrickColor = BrickColor.new("Really black")
                humanoidRootPart.Material = Enum.Material.Neon
                humanoidRootPart.CanCollide = false
            end
        end
    end
end

-- Connect the RenderStepped event to update the hitbox based on the toggle state
RunService.RenderStepped:Connect(function()
    if not _G.Disabled then
        updateHitbox()
    end
end)

-- TextBox to modify the hitbox size
Section6:NewTextBox("Hitbox modifier", "Enter a value to change the hitbox size.", function(txt)
    local newSize = tonumber(txt)
    if newSize and newSize > 0 then
        _G.HeadSize = newSize
        print("Hitbox Size Set to:", _G.HeadSize)
    else
        print("Invalid input for hitbox size. Please enter a positive number.")
    end
end)

-- Toggle to enable or disable the hitbox visibility
Section6:NewToggle("Invisible hitbox toggle", "Toggle to enable or disable the hitbox.", function(state)
    _G.Disabled = not state  -- Toggle the Disabled state
    if state then
        print("Hitbox Modifier Enabled")
    else
        print("Hitbox Modifier Disabled")
    end
end)


