local RunService = game:GetService("RunService")

RunService.RenderStepped:Connect(function(frame) -- This will fire every time a frame is rendered
    script.Parent.Text = ("FPS: " .. math.round(1 / frame)) 
end)

local function rgb()
    local counter = 0
    while true do
        counter = counter + 1
        local r = math.sin(counter / 20) * 127 + 128
        local g = math.sin(counter / 20 + 2 * math.pi / 3) * 127 + 128
        local b = math.sin(counter / 20 + 4 * math.pi / 3) * 127 + 128
        script.Parent.TextColor3 = Color3.fromRGB(r, g, b)
        wait(0.1)
    end
end

spawn(rgb)


