Section10:NewLabel("Play your own music")
Section10:NewTextBox("Enter song id", "Cool Right?", function(txt)
    local soundId = "rbxassetid://" .. tonumber(txt)

    local sound = Instance.new("Sound")
    sound.SoundId = soundId
    sound.Parent = game.Workspace 
    sound.Volume = volume / maxVolume

    sound:Play()
end)
Section10:NewLabel("Volume")
Section10:NewLabel("The max volume is 20")
Section10:NewLabel("The lowest volume is 0")
Section10:NewLabel("The starting volume is 10")

local volume = 10
local maxVolume = 20
local minVolume = 0


local function updateVolume()
    for _, sound in ipairs(game.Workspace:GetChildren()) do
        if sound:IsA("Sound") then
            sound.Volume = volume / maxVolume
        end
    end
end


Section10:NewButton("-", "Decrease Volume", function()
    if volume > minVolume then
        volume = volume - 1
        updateVolume()
        print("Volume decreased to", volume)
    else
        print("Volume is already at the minimum level")
    end
end)


Section10:NewButton("+", "Increase Volume", function()
    if volume < maxVolume then
        volume = volume + 1
        updateVolume()
        print("Volume increased to", volume)
    else
        print("Volume is already at the maximum level")
    end
end)

